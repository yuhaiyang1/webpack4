{
  "presets": [
    "react",
    "es2015",
    "stage-0"  
  ],
  "plugins": [
    "transform-runtime","transform-decorators-legacy", "react-hot-loader/babel"
  ],
  "env": {
    "develop": {
      "presets": [
        "react-hmre"
      ]
    },
    "develop_NoCommon": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "ignore": [
    "react",
    "react-dom",
    "antd"
  ]
}
